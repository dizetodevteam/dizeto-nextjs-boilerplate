    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
